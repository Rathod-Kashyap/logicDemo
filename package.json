    "mobx-persist": "0.4.1",
    "mobx-react": "7.6.0",
    "native-base": "3.4.25",
    "patch-package": "6.5.0",
    "react": "18.1.0",
    "react-native": "0.70.6",
    "react-native-code-push": "7.0.5",
    "react-native-gesture-handler": "1.10.3",
    "react-native-reanimated": "2.13.0",
